<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.yer0013.eShop.client.controller.Customer_Controller">
    <TableView fx:id="table" layoutX="14.0" layoutY="10.0" prefHeight="333.0" prefWidth="575.0">
      <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="priceColumn" prefWidth="75.0" text="Price" />
          <TableColumn fx:id="ratingColumn" onEditCommit="#ratingCommit" prefWidth="75.0" text="Rating" />
          <TableColumn fx:id="categoryColumn" prefWidth="75.0" text="Category" />
          <TableColumn fx:id="actionColumn" prefWidth="264.0" text="Action" />
      </columns>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
    <Button layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#initData" text="Refresh" />
</AnchorPane>
